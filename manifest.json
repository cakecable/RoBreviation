{
  "manifest_version": 3,
  "name": "Robreviation",
  "version": "1.0",
  "description": "This extension prevents abbreviations like K and M for Robux.",
  "permissions": ["activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
